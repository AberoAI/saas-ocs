// packages/shared/prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
  output   = "../../node_modules/.prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Tambahkan model jika sudah ada kebutuhan
model User {
  id    String @id @default(cuid())
  email String @unique
  name  String?
}
